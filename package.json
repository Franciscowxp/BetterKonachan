{
  "name": "Betterkonachan",
  "version": "4.0.0",
  "description": "",
  "private": true,
  "keywords": [
    "betterkonachan",
    "electron"
  ],
  "author": "franciscowxp@github.com",
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run client:build",
    "dev": "concurrently \"npm run start:dev\" \"npm run client:dev\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "client:dev": "cross-env NODE_ENV=development webpack serve --config ./src/webpack/config.ts",
    "client:build": "cross-env NODE_ENV=production webpack --config ./src/webpack/config.ts",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.4",
    "@nestjs/core": "^7.6.4",
    "@nestjs/platform-express": "^7.6.4",
    "@nestjs/serve-static": "^2.1.4",
    "axios": "^0.21.1",
    "better-react-spinkit": "^2.0.4",
    "cheerio": "1.0.0-rc.5",
    "mousetrap": "^1.6.5",
    "node-vibrant": "^3.2.1-alpha.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.3.4",
    "recoil": "^0.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "rxjs-retry-delay": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.6.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@snowpack/plugin-postcss": "^1.0.11",
    "@testing-library/react": "^11.2.2",
    "@types/cheerio": "^0.22.23",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/mousetrap": "^1.6.5",
    "@types/node": "^14.14.14",
    "@types/ramda": "^0.27.34",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.4.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.1",
    "postcss-assets": "^5.0.0",
    "postcss-cli": "^8.3.1",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react-refresh": "^0.9.0",
    "snowpack": "^2.18.5",
    "style-loader": "^2.0.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.13.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^3.0.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
